3d_render/layer_2="FPS_items"
3d_render/layer_3="Lights"
3d_render/layer_4="Shadows"
3d_render/layer_5="Ignore Light Source"
3d_physics/layer_1="Static"
3d_physics/layer_2="Player"
3d_physics/layer_3="Items"
3d_physics/layer_4="Interactibles"
3d_physics/layer_5="Flare"
3d_physics/layer_6="Darkness"
3d_physics/layer_7="Triggers"
3d_physics/layer_8="Shadow"
3d_physics/layer_9="Furniture"

[rendering]

textures/canvas_textures/default_texture_filter=0
occlusion_culling/use_occlusion_culling=true
occlusion_culling/quality/use_taa=false

