; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The Great Work"
run/main_scene="res://scenes/layers/splash_screen.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
run/max_fps=60
boot_splash/image="res://assets/images/start_black.png"
config/icon="res://assets/images/icon.svg"

[autoload]

GameManager="*res://scripts/globals/game_manager.gd"
Events="*res://scripts/globals/events.gd"
SfxManager="*res://scripts/globals/sfx_manager.gd"
MusicManager="*res://scripts/music_manager.gd"
DialogManager="*res://scripts/globals/dialog_manager.gd"
TransitionManager="*res://scripts/globals/transition_manager.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
RuntimeDebugTools="*res://addons/runtime_debug_tools/scenes/remote_inspector.tscn"
EmbedGameAutoload="*res://addons/fabimakesgames.embed_game/embed_game_autoload.gd"
Shaker="*res://addons/shaker/src/Shaker.gd"
Panku="*res://addons/panku_console/console.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/initial_position_type=0
window/size/resizable=false
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="The Great Work"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/assetplacer/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/fabimakesgames.embed_game/plugin.cfg", "res://addons/label_font_auto_sizer/plugin.cfg", "res://addons/panku_console/plugin.cfg", "res://addons/runtime_debug_tools/plugin.cfg", "res://addons/shaker/plugin.cfg", "res://addons/ultimate_character/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "red",
"res://assets/": "orange",
"res://scenes/": "green",
"res://scripts/": "purple"
}

[global_group]

player=""
ingredient=""
shadow_spawn_points=""
flare=""
shadow=""
darkness=""
dispenser=""
candle=""

[gui]

theme/custom_font="res://assets/ui/fonts/cruft.ttf"

[importer_defaults]

scene={
"import_script/path": "res://addons/blender_godot_pipeline/GLTFImporter.gd"
}
wav={
"compress/mode": 1
}

